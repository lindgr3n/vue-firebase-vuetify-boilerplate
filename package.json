{
  "name": "vue-firebase-vuetify-boilerplate",
  "description": "Up and running with Vue, Vuetify and Firebase",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Johan Lindgren"
  },
  "bugs": {
    "url": "https://github.com/lindgr3n/vue-firebase-vuetify-boilerplate/issues"
  },
  "keywords": [
    "Vue", "Vuetify", "Firebase", "vue-cli", "Cloud Firestore"
  ],
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "firebase:login": "firebase login",
    "firebase:init": "firebase init",
    "firebase:deploy": "firebase deploy",
    "deploy": "yarn build && yarn firebase:deploy"
  },
  "dependencies": {
    "@sentry/browser": "^5.15.0",
    "@sentry/integrations": "^5.15.0",
    "core-js": "^3.6.4",
    "firebase": "^7.12.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-e2e-cypress": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.0",
    "@vue/cli-plugin-vuex": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "firebase-tools": "^7.15.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
